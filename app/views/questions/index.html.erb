<h1>All Questions</h1>

<ul>
<%@questions.each do |question|%>
  <li>
    <%= link_to question.title, question_path(question) %>
  </li>
<%end%>
</ul>

<%= form_for :question do |f| %>
  Your Name: <%= f.text_field :asker %><br />
  Your Question: <%= f.text_field :title %><br />
  Description: <%= f.text_area :body %><br />
  <%= f.submit %>
<% end %>
